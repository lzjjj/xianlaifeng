/**app.wxss**/
@import "style/weui.wxss";
 
.container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  background-color: #f5f5f5;
}

.personal_head {
  width: 100%;
  height: 31.5%;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.userinfo-avatar {
  width: 26%;
  height: 60%;
  border-radius: 50%;
}

.arrow_img {
  width: 1rem;
  height: 1rem;
  margin-right: -3%;
  vertical-align: middle;
  margin-top: 1%;
}

.placeholder_style {
  font-size: 0.8125rem;
  padding-left: 1.65rem;
  color: rgba(153, 153, 153, 1) !important;
}

/*控制字体一行显示，超出用省略号表示  */

.item_title {
  width: 99%;
  overflow: hidden; /*超出部分隐藏*/
  white-space: nowrap; /*不换行*/
  text-overflow: ellipsis; /*超出部分文字以...显示*/
}

.scroll_content {
  width: 100%;
  height: 50%;
}

.scroll_item {
  width: 100%;
  display: flex;
  flex-direction: row;
  padding: 2%;
  border-bottom: 1px #f5f5f5 solid;
}

.pay_way_img {
  width: 1.75rem;
  height: 0.93rem;
  vertical-align: middle;
  margin-left: 0.4375rem;
}

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
}

.search_img {
  background: url("data:image/png;base64,") no-repeat;
  background-size: 0.84rem 0.84rem;
  background-position: 0.3125rem 50%;
  background-color: rgba(245, 245, 245, 1);
}

.time_icon {
  width: 0.8rem;
  height: 0.8rem;
  vertical-align: middle;
  margin-right: 2%;
  margin-left: 2%;
}

.no_content {
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size:13rem 13rem;
  /* background-size: cover;   */
  /* -moz-background-size: 80% 50%;
  -o-background-size: 80% 50%;
  -webkit-background-size: 80% 50%; */
  background-position: 50% 5rem;
  width: 100%;
  height: 580px;
  overflow-y: hidden;
   /* position: r;  */
}
.button_notallow {
  width: 70%;
  background-color: #f5f5f5;
  color: gainsboro;
  margin-right: 0.9rem;
}

.button_allow {
  width: 36%;
  line-height: 2.5rem;
  background-color: #fcd917;
  color: white;
}
